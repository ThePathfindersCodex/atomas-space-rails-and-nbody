; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Ellipse",
"language": "GDScript",
"path": "res://Ellipse.gd"
}, {
"base": "Node2D",
"class": "MassBody",
"language": "GDScript",
"path": "res://MassBody.gd"
}, {
"base": "Node2D",
"class": "Orb",
"language": "GDScript",
"path": "res://Orb.gd"
}, {
"base": "Node2D",
"class": "Star",
"language": "GDScript",
"path": "res://Star.gd"
}, {
"base": "Node2D",
"class": "Trajectory",
